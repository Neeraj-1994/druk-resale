{
  "version": 1,
  "projects": {
    "druk-resale": {
      "projectType": "application",
      "root": "apps/druk-resale",
      "sourceRoot": "apps/druk-resale/src",
      "prefix": "druk-resale",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/druk-resale",
            "index": "apps/druk-resale/src/index.html",
            "main": "apps/druk-resale/src/main.ts",
            "polyfills": "apps/druk-resale/src/polyfills.ts",
            "tsConfig": "apps/druk-resale/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/druk-resale/src/favicon.ico",
              "apps/druk-resale/src/assets"
            ],
            "styles": [
              "apps/druk-resale/src/assets/styles/bootstrap.scss",
              "apps/druk-resale/src/assets/styles/app.scss",
              "apps/druk-resale/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/druk-resale/src/environments/environment.ts",
                  "with": "apps/druk-resale/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "druk-resale:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "druk-resale:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "druk-resale:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/druk-resale/src/**/*.ts",
              "apps/druk-resale/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/druk-resale"
          ],
          "options": {
            "jestConfig": "apps/druk-resale/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "druk-resale-e2e": {
      "root": "apps/druk-resale-e2e",
      "sourceRoot": "apps/druk-resale-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/druk-resale-e2e/cypress.json",
            "tsConfig": "apps/druk-resale-e2e/tsconfig.e2e.json",
            "devServerTarget": "druk-resale:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "druk-resale:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/druk-resale-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "feature-home": {
      "projectType": "library",
      "root": "libs/feature/home",
      "sourceRoot": "libs/feature/home/src",
      "prefix": "home",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature/home/src/**/*.ts",
              "libs/feature/home/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/feature/home"
          ],
          "options": {
            "jestConfig": "libs/feature/home/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-post-product": {
      "projectType": "library",
      "root": "libs/feature/post-product",
      "sourceRoot": "libs/feature/post-product/src",
      "prefix": "pp",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature/post-product/src/**/*.ts",
              "libs/feature/post-product/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/feature/post-product"
          ],
          "options": {
            "jestConfig": "libs/feature/post-product/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-product-order": {
      "projectType": "library",
      "root": "libs/feature/product-order",
      "sourceRoot": "libs/feature/product-order/src",
      "prefix": "po",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature/product-order/src/**/*.ts",
              "libs/feature/product-order/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/feature/product-order"
          ],
          "options": {
            "jestConfig": "libs/feature/product-order/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-auth": {
      "projectType": "library",
      "root": "libs/feature/auth",
      "sourceRoot": "libs/feature/auth/src",
      "prefix": "auth",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature/auth/src/**/*.ts",
              "libs/feature/auth/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/feature/auth"
          ],
          "options": {
            "jestConfig": "libs/feature/auth/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-components": {
      "projectType": "library",
      "root": "libs/shared-components",
      "sourceRoot": "libs/shared-components/src",
      "prefix": "sc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-components/src/**/*.ts",
              "libs/shared-components/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared-components"
          ],
          "options": {
            "jestConfig": "libs/shared-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "druk-resale"
}
